{
  "name": "LiveTraffic-Testing",
  "version": "1.0.0",
  "description": "To demonstrate e2e tests for LiveTraffic.com",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "npm run clean",
    "clean": "rimraf target",
    "e2e": "npm run clean && protractor protractor.config.js --verbose && serenity run",
    "browserstack": "npm run clean && protractor browserstack.conf.js",
    "e2e:only": "npm run clean && protractor protractor.config.js --cucumberOpts.tags '@only' --verbose",
    "webdriver-update": "webdriver-manager update --standalone --versions.standalone 3.0.1",
    "webdriver-start": "webdriver-manager start",
    "serenity:update": "serenity update",
    "serenity:run": "serenity run"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "moment-timezone": "0.5.26",
    "protractor-cucumber-framework": "^6.2.0"
  },
  "devDependencies": {
    "@types/cucumber": "^1.0",
    "@types/node": "^10.7.1",
    "@types/selenium-webdriver": "^3.0.10",
    "all-contributors-cli": "^6.1.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^6.2.0",
    "mkdirp": "^0.5.1",
    "query-string": "^6.8.3",
    "rimraf": "^2.6.2",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "latest",
    "cucumber": "1.3",
    "npm-failsafe": "0.4.1",
    "ts-node": "~7.0.0",
    "typescript": "~3.5.3",
    "cucumber-tsflow": "^3.2.0",
    "protractor": "~5.4.0",
    "protractor-flake": "^3.0.1",
    "serenity-cli": "^0.11.3",
    "serenity-js": "^1.10.13"
  }
}
